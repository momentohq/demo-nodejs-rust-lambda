{
  "name": "demo-pure-nodejs-lambda",
  "version": "1.0.0",
  "description": "A Little Rust-y Demo: Pure Node.js lambda",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prebuild": "eslint . --ext .ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "local-ingest": "tsc && node dist/local-ingest.js",
    "local-ingest-rust": "tsc && node dist/local-ingest-rust.js",
    "flush-cache": "tsc && node dist/flush-cache.js",
    "sample-cache-data": "tsc && node dist/sample-cache-data.js"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.118",
    "@types/node": "^18.19.55",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@gomomento/sdk": "1.99.0",
    "aws-lambda": "1.0.7",
    "dotenv": "16.4.5",
    "napi_rs_momento_cache": "file:../rust-workspace/napi_rs_momento_cache",
    "winston": "3.15.0"
